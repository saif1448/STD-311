<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.smarthomedesktopapp.controller.HomeViewController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#e5eaef" height="308.0" layoutX="67.0" layoutY="46.0" stroke="BLACK" strokeType="INSIDE" width="454.0" />
      <Button fx:id="toggle_light_btn" layoutX="228.0" layoutY="107.0" mnemonicParsing="false" onAction="#handleToggleLightBtn" text="Toggle Light">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="inc_temp_btn" layoutX="196.0" layoutY="180.0" mnemonicParsing="false" onAction="#handleIncTempBtn" text="Increase Temperature">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="dec_temp_btn" layoutX="193.0" layoutY="253.0" mnemonicParsing="false" onAction="#handleDecTempBtn" text="Decrease Temperature">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
